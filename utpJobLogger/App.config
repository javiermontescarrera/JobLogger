<?xml version="1.0" encoding="utf-8" ?>
<configuration>
      <connectionStrings>
        <add name="BDJobLogger"
            connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\BDLogger.mdf;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
  <appSettings>
    <add key="LogFileDirectory" value="C:\Logs\"/>
    <add key="MaxMessageSize" value="500"/>
  </appSettings>
</configuration>